    const hambúrguer = documento.querySelector(".hamburger");
    const navMenu = documento.querySelector(".nav-menu");

    hambúrguer.addEventListener("clique", mobliemmenu);

    função mobliemmenu() {
      hambúrguer.classList.alternar("ativo");
      navMenu.classList.alternar("ativo");
    }

    janela.addEventListener("rolar", função() {
      var cabeçalho = documento.querySelector("cabeçalho");
      cabeçalho.classList.alternar("pegajoso", janela.rolagem > 0)
    })
  </roteiro>


